{
"apiVersion": "v1",
"kind": "Pod",
"metadata": {
            "name": "monitor-d-new",
            "labels": {
              "name":"monitor-d"
            }
           },
            "spec": {
                    "containers": [
                          {
                          "name": "monitor-d-ui",
                          "image": "docker.io/bzcareer/monitord",
                          "ports": [
                                  {
                                  "containerPort": 8080
                                  }]
                          },
                          {
                          "name": "monitor-d-backend",
                          "image": "docker.io/bzcareer/monitord-core-api",
                          "ports": [
                                  {
                                  "containerPort": 8181
                                  }]
                          },
                          {
                          "name": "monitor-d-database",
                          "image": "docker.io/mongo",
                          "ports": [
                                  {
                                  "containerPort": 27017
                                  }]
			  }]



                    }
}
